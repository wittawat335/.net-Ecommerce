@using Ecommerce.Web.Commons;
@using Ecommerce.Web.Models;
@using Ecommerce.Web.Models.Position;
@model PositionViewModel

<form id="frmDetail" role="form">
    @{
        await Html.RenderPartialAsync("_FormDetail", Model.Position);
    }
    <div style="display: none">
        <input type="text" name="action" value="@Model.Action"/>
    </div>
</form>
@{
    await Html.RenderPartialAsync("_PopUpPositionScripts", Model);
}